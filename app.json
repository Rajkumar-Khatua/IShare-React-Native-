{
  "expo": {
    "name": "IShare",
    "slug": "ishare",
    "scheme": "ishare",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ishare.app"
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "plugins": ["expo-router"]
  }
}
